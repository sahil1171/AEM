List<UserGroupReport> sortedUserGroupReports = userGroupReports.stream()
    .sorted(Comparator.comparing(UserGroupReport::getPrincipalName))
    .toList();



 List<UserGroupReport> sortedUserGroupReports = userGroupReports.stream()
                .sorted(Comparator.comparing(UserGroupReport::getPrincipalName))
                .collect(Collectors.toList());
